@model Richinoz.Paypal.Models.OrderPart
@{
    Script.Require("jQuery");
    
}
<fieldset>
    <legend>@T("Details")</legend>
    @Html.LabelFor(m => m.Id, T("Id"))
    @Html.TextBoxFor(m => m.Id)
    @Html.LabelFor(m => m.TransactionId, T("TransactionId"))
    @Html.TextBoxFor(m => m.TransactionId)
    @Html.LabelFor(m => m.Amount, T("Amount"))
    @Html.TextBoxFor(m => m.Amount)
    @Html.LabelFor(m => m.Details, T("Details"))
    @Html.TextBoxFor(m => m.Details)
    @{
        @Html.Label("Name", T("Name"))
        foreach (var orderItem in Model.Order.OrderItems)
        {
        @Html.Label(orderItem.Name)
        }
    }
    <fieldset class="contentItems bulk-items">
        <ul>
            <li>
                <div class="summary">
                    <div class="properties">
                        Address
                        <div class="metadata">
                            @Html.LabelFor(m => m.Order.Address, T("Street"))
                            @Html.TextBoxFor(m => m.Order.Address.Street1)
                            City
                              @*@Html.LabelFor(m => m.Order.Address, T("City"))*@
                @Html.TextBoxFor(m => m.Order.Address.City)
                @Html.LabelFor(m => m.Order.Address, T("Postcode"))
                @Html.TextBoxFor(m => m.Order.Address.Zip)
                        </div>
                    </div>
                </div>
                test </li>
        </ul>
        <div class="summary">
            <div class="properties">
                
                @Html.LabelFor(m => m.Order.Address, T("City"))
                @Html.TextBoxFor(m => m.Order.Address.City)
                @Html.LabelFor(m => m.Order.Address, T("Postcode"))
                @Html.TextBoxFor(m => m.Order.Address.Zip)
            </div>
        </div>
    </fieldset>
</fieldset>
