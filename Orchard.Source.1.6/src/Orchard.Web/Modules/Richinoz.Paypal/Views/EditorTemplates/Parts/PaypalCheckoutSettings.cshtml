@model Richinoz.Paypal.Models.PaypalCheckoutSettingsPartRecord
<fieldset>
    <label class="sub" for="MerchantId">@T("Paypal business email")</label><br />
    @Html.TextBoxFor(m => m.MerchantId, new { @class = "text" })<br />
    <label class="sub" for="AnalyticsId">@T("Paypal Analytics Id")</label><br />
    @Html.TextBoxFor(m => m.AnalyticsId, new { @class = "text" })<br />
    <label class="sub" for="Currency">@T("Currency")</label><br />
    @Html.TextBoxFor(m => m.Currency, new { @class = "text" })<br />
    <label class="sub" for="WeightUnit">@T("Shipping Weight Unit")</label><br />
    @Html.TextBoxFor(m => m.WeightUnit, new { @class = "text" })<br />
    
    @Html.CheckBoxFor(m => m.UseSandbox)
    <label class="sub" for="UseSandbox">@T("Use Sandbox")</label><br />
    
    <label class="sub" for="ReturnUrl">@T("return URL")</label><br />
    @Html.TextBoxFor(m => m.ReturnUrl, new { @class = "text" })<br />
</fieldset>